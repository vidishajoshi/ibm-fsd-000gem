package comm.example.employee;

import java.util.List;
import java.util.ArrayList;

public class Testing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 List<Employee> list = new ArrayList<Employee>();
		
		list.add(new Employee("sum","dosi",28,89706));
		
		list.add(new Employee("susidhm","ddkjosi",28,98606));
		list.add(new Employee("susidhm","ddkjosi",28,806));
		
		int count=1;
		for(Employee o:list) {
		    
			System.out.printf("%-15s %-30s %-30s %-10s %-10s\n",count,o.getName(),o.getDepartment(),o.getAge(),o.getSalary());
		count++;
		}
		
	}

}
