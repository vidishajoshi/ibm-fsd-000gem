package comm.example;
import comm.expample.client.MyDate;

public class Tester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        MyDate myDay = new MyDate();
        int val = myDay.getDay(23);
        myDay.setMonth(4);
        myDay.setYear(1997);
        System.out.println("Day : "+val);
        System.out.println();
	}

}
