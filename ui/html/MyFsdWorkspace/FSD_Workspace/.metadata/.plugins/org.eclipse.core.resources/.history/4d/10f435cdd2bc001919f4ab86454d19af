package comm.expample.client;

public class MyDate {
	private int day;
	private int month;
	private int year;
	
	public int getDay(int d) {
		if(d>30) {
			System.out.println("Day can't be greater than 30");
			System.exit(0);
		}
		return d;
	}
	
	public int getMonth(int m) {
		if(m>12) {
			System.out.println("month can't be greater than 12");
			System.exit(0);
		}
		return m;
	}
	
	public int getYear(int y) {
		if(y>2019 && y<1990) {
			System.out.println("wrong year");
			System.exit(0);
		}
		return y;
	}
	
	public boolean setDay(int d) {
		day 
		return true;
	}
	public boolean setMonth(int m) {
		month = m;
		return true;
	}
	public boolean setYear(int y) {
	   year = y;
		return true;
	}
	

}
