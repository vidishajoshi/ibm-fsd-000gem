package beans;

public class CricketCoach implements Coach {

	private Address add;
	public String getDailyWorlkout() {
		
		return "do exercise";
	
	}
	
	

	public CricketCoach(Address add) {
		super();
		this.add = add;
	}



	public String getAddress() {
		// TODO Auto-generated method stub
		return add.getAddress();
	}

	public int getPhone() {
		// TODO Auto-generated method stub
		return add.getPhone();
	}

	public String getCityState() {
		// TODO Auto-generated method stub
		return add.getCityState();
	}

}
